{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  
  {
    "rewrites": [
      { "source": "/api/:path*", "destination": "/api/:path*" }
    ],
    "functions": {
      "api/stripe-webhook.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }